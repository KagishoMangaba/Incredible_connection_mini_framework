<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Selenium Grid Docker Suite" parallel="tests" thread-count="2">

    <!-- Parameters passed to your framework -->
    <parameter name="execution" value="grid"/>
    <parameter name="browser" value="chrome"/>
    <parameter name="gridUrl" value="http://localhost:4444/wd/hub"/>


    <test name="UI Tests on Chrome (Docker Grid)">
        <classes>
            <class name="kagishomangaba.tests.regression.OrderTest"/>

        </classes>
    </test>

</suite>
